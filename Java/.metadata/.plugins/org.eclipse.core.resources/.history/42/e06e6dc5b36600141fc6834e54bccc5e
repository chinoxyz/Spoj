import java.io.BufferedReader;
import java.io.FileReader;
import java.math.BigInteger;


class COINTOSS {

	public static void main(String[] args) {
		try
		{
			BufferedReader br = null;
			br = new BufferedReader(new FileReader(".\\Text\\COINTOSS.txt"));
			//br = new BufferedReader(new InputStreamReader(System.in));
			
			int cases=Integer.parseInt(br.readLine());
			
			
			while(cases-- >0)
			{
				
				int n=(int)Integer.parseInt(br.readLine());
				System.out.println("DD"+cases);
				int m=(int)Integer.parseInt(br.readLine());
				System.out.println("DD"+n+m);
				
				if(n<=m)
				{
					System.out.println("0.00");
					//
				}
				else
				{
					
					BigInteger twoo=new BigInteger("2");
					BigInteger a=twoo.pow(n+1);
					BigInteger b=twoo.pow(m+1);
					System.out.print(a.subtract(b));
					System.out.println(".00");
				}
				
			}
			

			
		}
		catch(Exception e)
		{
			
		}

	}

}
